    <section class="site-blocks-cover overflow-hidden">
        <div class="container">
            <div class="row mb-5 justify-content-center">
                <div class="col-md-7 align-self-center">
                    <style>
                        div.polaroid {
                            width: 100%;
                            background-color: white;
