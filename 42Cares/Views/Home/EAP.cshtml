<style>
    img {
        float: right;
        max-height: 500px;
        overflow: auto;
        border: 5px solid white;
    }

    .img2 {
        float: left;
        border: 5px solid white;
    }
    
    div.container {
        text-align: center;
        padding: 10px 20px;
        font-size: 18px;
    }

    body {
        font-family: 'Times New Roman', Times, serif background-color: #2ee7ff;
        background: -webkit-linear-gradient(top right, #525994, #000000);
        background: -moz-linear-gradient(top right, #525994, #000000);
        background: linear-gradient(to bottom left, #525994, #000000);
        background-attachment: fixed;
        background-size: cover;
        background-attachment: fixed;
        background-size: cover;
        color: white;
        font-size: 18px;
    }

    .list-group-item {
        position: relative;
        display: block;
        padding: 10px 15px;
        margin-bottom: -1px;
        margin-right: 15px;
        background-color: #a6a7a7;
        border: 1px solid #000;
        
       
    }

    /* The Modal (background) */
    .modal {
        display: none; /* Hidden by default */
        position: fixed; /* Stay in place */
        z-index: 1; /* Sit on top */
        padding-top: 100px; /* Location of the box */
        left: 0;
        top: 0;
        width: 100%; /* Full width */
        height: 100%; /* Full height */
        overflow: auto; /* Enable scroll if needed */
        background-color: rgb(0,0,0); /* Fallback color */
        background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
    }

    /* Modal Content */
    .modal-content {
        background-color: ##e5e8e1;
        margin: auto;
        padding: 20px;
        border: 1px solid #888;
        width: 60%;
        color: black;
    }

    /* The Close Button */
    .close {
        color: #aaaaaa;
        float: right;
        font-size: 28px;
        font-weight: bold;
    }

        .close:hover,
        .close:focus {
            color: #000;
            text-decoration: none;
            cursor: pointer;
        }
</style>

